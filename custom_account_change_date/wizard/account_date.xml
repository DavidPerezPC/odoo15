<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
       <record id="wizard_account_change_date_form" model="ir.ui.view">
           <field name="name">wizard.account.change.date.form</field>
           <field name="model">account.change.date</field>
           <field name="arch" type="xml">
               <form string="Cambio de fecha">
                   <sheet>
                       <group>
                           <group>
                               <field name="name" readonly="1"/>
                               <field name="move_id" invisible="1"/>
                           </group>
                           <group>
                               <field name="date"/>
                           </group>
                       </group>
                   </sheet>
                   <footer>
                        <button name="change_date" string="Cambiar fecha" type="object" class="btn btn-primary"/>
                        <button string="Cerrar" class="btn-secondary" special="cancel"/>
                    </footer>
               </form>
           </field>
       </record>
    </data>
</odoo>