<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <template id="modify_report_delivery" inherit_id="stock.report_delivery_document">
            <xpath expr="//div[1]/div[1]/div[1]" position="after">
                <div t-if="o.state" class="col-auto" name="div_state">
                    <strong>Estado:</strong>
                    <p t-field="o.state"/>
                </div>
            </xpath>
        </template>

    </data>
</odoo>